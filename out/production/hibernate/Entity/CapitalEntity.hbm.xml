<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entity.CapitalEntity" table="CAPITAL" schema="SYSTEM">
        <id name="capital">
            <column name="CAPITAL" sql-type="varchar2(30)" length="30"/>
        </id>
        <property name="governor">
            <column name="GOVERNOR" sql-type="varchar2(30)" length="30" not-null="true"/>
        </property>
        <property name="population">
            <column name="POPULATION" sql-type="number(*)" precision="2147483646" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>